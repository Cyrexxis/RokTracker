{
  "name": "rok-tracker-suite",
  "version": "0.0.1",
  "description": "A Quasar Project",
  "productName": "Rok Tracker Suite",
  "author": "Cyrexxis <cyrexxis@outlook.com>",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build",
    "build:watch": "quasar build --debug",
    "build:app": "yarpm run build && .\\venv\\Scripts\\pyinstaller windows.spec",
    "postinstall": "quasar prepare",
    "start:app": "yarpm run build:watch && yarpm run run:py",
    "dev:py": ".\\venv\\Scripts\\python -X dev .\\main.py",
    "run:py": ".\\venv\\Scripts\\python .\\main.py",
    "generate:zod": "tsx node_scripts/generateZod.ts",
    "generate:json-schema": ".\\venv\\Scripts\\python -X dev .\\schema_generator.py"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.4",
    "@vueuse/components": "^12.4.0",
    "pinia": "^2.0.11",
    "quasar": "^2.16.0",
    "theme-toggles": "^4.10.1",
    "vue": "^3.4.18",
    "vue-router": "^4.0.12",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@quasar/app-vite": "^2.0.0",
    "@types/node": "^20.5.9",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vueuse/core": "^12.4.0",
    "autoprefixer": "^10.4.2",
    "change-case": "^5.4.4",
    "concurrently": "^9.1.0",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "globals": "^15.12.0",
    "json-refs": "^3.0.15",
    "json-schema-to-zod": "^2.6.0",
    "prettier": "^3.3.3",
    "tsx": "^4.19.2",
    "typescript": "~5.5.3",
    "vite-plugin-checker": "^0.8.0",
    "vue-tsc": "^2.0.29",
    "yarpm": "^1.2.0"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}
